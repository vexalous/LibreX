QPushButton {
    background-color: #333;
    color: white;
    border: 1px solid #555;
    border-radius: 10px;
    padding: 5px 10px;
    font-size: 14px;
}

QPushButton:hover {
    background-color: #444;
}

QPushButton:pressed {
    background-color: #555;
}

QPushButton:focus {
    border: 1px solid #0078d7;
}
